<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
    <!-- 配置缓存机制，使用延迟加载 -->
    <settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" errorTracingEnabled="true" maxRequests="300"
		maxSessions="100" maxTransactions="50" useStatementNamespaces="true" />
    
    
	<sqlMap resource="ibatis/mapping/Users.xml" />
	<sqlMap resource="ibatis/mapping/Priviliege.xml" />
	<sqlMap resource="ibatis/mapping/Role.xml" />
	<sqlMap resource="ibatis/mapping/Project.xml" />
	<sqlMap resource="ibatis/mapping/Field.xml" />
	<sqlMap resource="ibatis/mapping/FieldWarm.xml" />
	<sqlMap resource="ibatis/mapping/Subject.xml" />
	<sqlMap resource="ibatis/mapping/Report.xml" />
	<sqlMap resource="ibatis/mapping/ReportDetail.xml" />
	<sqlMap resource="ibatis/mapping/Bill.xml" />
	<sqlMap resource="ibatis/mapping/BillDetail.xml" />
	<sqlMap resource="ibatis/mapping/Message.xml" />
	<sqlMap resource="ibatis/mapping/Inventory.xml" />
</sqlMapConfig>